{
  "name": "coverage-diff-back",
  "version": "0.1.0",
  "description": "Send the coverage difference back to the github on each pull requests",
  "license": "MIT",
  "repository": "9renpoto/coverage-diff-back",
  "author": [
    "Leko <leko.noor@gmail.com>",
    "9renpoto"
  ],
  "homepage": "https://github.com/9renpoto/coverage-diff-back#readme",
  "bugs": {
    "url": "https://github.com/9renpoto/coverage-diff-back/issues"
  },
  "bin": {
    "coverage-diff-back": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "commit": "git-cz",
    "prepare": "yarn build",
    "test": "jest"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "ci",
    "test",
    "coverage",
    "report",
    "github",
    "pull request"
  ],
  "dependencies": {
    "@octokit/graphql": "^4.5.4",
    "@octokit/rest": "^18.0.4",
    "deindent": "^0.1.0",
    "env-ci": "^5.0.2",
    "glob": "^7.1.3",
    "node-fetch": "^2.3.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@types/glob": "7.1.1",
    "@types/jest": "26.0.9",
    "@types/node-fetch": "2.1.4",
    "@types/yargs": "15.0.5",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "husky": "4.2.5",
    "jest": "26.4.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "prettier-plugin-organize-imports": "1.1.1",
    "prettier-plugin-package": "1.0.0",
    "ts-jest": "26.2.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md,yml}": [
      "prettier --write"
    ]
  }
}
